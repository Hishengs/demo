{
  "name": "npm-workspace",
  "version": "1.0.0",
  "lockfileVersion": 2,
  "requires": true,
  "packages": {
    "": {
      "name": "npm-workspace",
      "version": "1.0.0",
      "license": "ISC",
      "workspaces": [
        "packages/*"
      ]
    },
    "node_modules/logger": {
      "resolved": "packages/logger",
      "link": true
    },
    "node_modules/monitor": {
      "resolved": "packages/monitor",
      "link": true
    },
    "packages/logger": {
      "version": "1.0.0",
      "license": "ISC"
    },
    "packages/monitor": {
      "version": "1.0.0",
      "license": "ISC"
    }
  },
  "dependencies": {
    "logger": {
      "version": "file:packages/logger"
    },
    "monitor": {
      "version": "file:packages/monitor"
    }
  }
}
